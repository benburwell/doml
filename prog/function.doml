statement-sequence {
	assign(name=myFunc) {
		function(args=stringToPrint,val1,val2) {
			print {
				variable(name=stringToPrint)
				bin-op(op=+) {
					variable(name=val1)
					variable(name=val2)
				}
			}
		}
	}

	call(name=myFunc) {
		constant(val="hello, world!" type="string")
		constant(val="3.5" type="number")
		constant(val="7" type="number")
	}
}
